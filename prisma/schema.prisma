generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model StoreStatusLogs {
  id         Int      @id @default(autoincrement())
  createdAt  DateTime @default(now())
  lastOnline DateTime
  brand      String   @db.VarChar(255)
  storeId    Int
}
